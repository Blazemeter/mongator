    "keywords": ["odm", "mongodb"],
    "homepage": "http://mandango.org",
    "license": "MIT",
    "authors": [
        {
            "name":  "Máximo Cuadros",
            "email": "maximo@yunait.com"
        },
        {
            "name":  "Daniel Ripollés",
            "email": "daniel@yunait.com"
        },
        {
            "name":  "Pablo Díez",
            "email": "pablodip@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "yunait/mondator": "dev-master"
    },
    "replace": {
        "mandango/mandango": "dev-master"
    },
    "suggest": {
        "ext-apc": "Allow caching on APC",
