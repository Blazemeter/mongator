<?php

    /**
     * Translatable fields
     */
    public $translateFields=[
{% if config_class.translateFields is defined %}
{% for tField in config_class.translateFields %}
        '{{ tField }}',
{% endfor %}
{% endif %}
    ];



    /**
     * {@inheritdoc}
     */
    public function __construct($mongator = null)
    {
        if ($mongator === null) {
          $mongator = mandango_get();
        }
        $this->eventPattern = '{{ config_class.eventPattern }}';

        parent::__construct($mongator);
    }
